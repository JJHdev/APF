/* Create Tables */

-- 업체정보
CREATE TABLE TB_ENT
(
	BZENTY_NO varchar(20) NOT NULL,
	UP_BZENTY_NO varchar(20),
	BZENTY_SE_CD varchar(3),
	BRNO char(10) UNIQUE,
	CRNO char(13),
	BZENTY_NM varchar(100),
	RPRSV_NM varchar(20),
	FNDN_YMD varchar(8),
	RPRS_TELNO varchar(11),
	FXNO varchar(20),
	TPBIZ_CD varchar(10),
	TPBIZ_NM varchar(200),
	BZENTY_TYPE_CD varchar(3),
	BZENTY_STLE_CD varchar(3),
	BZENTY_SCALE varchar(100),
	INVT_HOPE_AMT numeric(15),
	EML_ADDR varchar(100),
	HMPG_ADDR varchar(100),
	ZIP char(5),
	LCTN_ADDR varchar(200),
	LCTN_DADDR varchar(200),
	EMP_CNT numeric(10),
	MNGR_MEMO varchar(300),
	APRV_YMD timestamp,
	RJCT_YMD timestamp,
	USE_STTS_CD varchar(3),
	LCINFO point,
	RGTR_NO varchar(20),
	REG_YMD timestamp,
	MDFR_NO varchar(20),
	MDFCN_YMD timestamp,
	PRIMARY KEY (BZENTY_NO)
) WITHOUT OIDS;


-- 업체첨부파일
CREATE TABLE TB_ENT_FILE
(
	BZENTY_NO varchar(20) NOT NULL,
	SN numeric(10) NOT NULL,
	DOC_SE_CD varchar(3),
	FILE_NM varchar(200),
	STRG_FILE_NM varchar(200),
	FILE_PATH varchar(200),
	FILE_SZ numeric(14),
	RPRS_YN char(1),
	DEL_YN char(1),
	RGTR_NO varchar(20),
	REG_YMD timestamp,
	MDFR_NO varchar(20),
	MDFCN_YMD timestamp,
	PRIMARY KEY (BZENTY_NO, SN)
) WITHOUT OIDS;


-- 재무정보(KODATA)
CREATE TABLE TB_ENT_FNNR_KO
(
	BZENTY_NO varchar(20) NOT NULL,
	FNNR_ACNT_CD varchar(10) NOT NULL,
	FNNR_YMD varchar(8),
	FNNR_AMT numeric(15),
	RGTR_NO varchar(20),
	REG_YMD timestamp,
	MDFR_NO varchar(20),
	MDFCN_YMD timestamp,
	PRIMARY KEY (BZENTY_NO, FNNR_ACNT_CD)
) WITHOUT OIDS;


-- 업체조회이력
CREATE TABLE TB_ENT_INQ_HST
(
	SN numeric(10) NOT NULL,
	USER_NO varchar(20),
	INQ_YMD varchar(8),
	REG_YMD timestamp,
	PRIMARY KEY (SN)
) WITHOUT OIDS;


-- 특허상표권현황(KODATA)
CREATE TABLE TB_ENT_PTNT_KO
(
	BZENTY_NO varchar(20) NOT NULL,
	SN numeric(10) NOT NULL,
	SE_CD varchar(3),
	APPLNM varchar(20),
	PATNTRT_MAN varchar(20),
	NM varchar(100),
	ILLT_REG_NO varchar(20),
	PATENT_REG_YMD varchar(8),
	RGTR_NO varchar(20),
	REG_YMD timestamp,
	MDFR_NO varchar(20),
	MDFCN_YMD timestamp,
	PRIMARY KEY (BZENTY_NO, SN)
) WITHOUT OIDS;


-- 업체분야정보
CREATE TABLE TB_ENT_RLM
(
	BZENTY_NO varchar(20) NOT NULL,
	FLD_SE_CD varchar(3) NOT NULL,
	FLD_CD varchar(10) NOT NULL,
	FLD_CN varchar(4000),
	RGTR_NO varchar(20),
	REG_YMD timestamp,
	MDFR_NO varchar(20),
	MDFCN_YMD timestamp,
	PRIMARY KEY (BZENTY_NO, FLD_SE_CD, FLD_CD)
) WITHOUT OIDS;


-- 재무계정코드(KODATA)
CREATE TABLE TB_FNNR_CODE_KO
(
	FNNR_ACNT_CD varchar(10) NOT NULL,
	UP_FNNR_ACNT_CD varchar(10),
	FNNR_SE_CD varchar(3),
	FNNR_ACNT_NM varchar(100),
	UNIT varchar(20),
	RGTR_NO varchar(20),
	REG_YMD timestamp,
	MDFR_NO varchar(20),
	MDFCN_YMD timestamp,
	PRIMARY KEY (FNNR_ACNT_CD)
) WITHOUT OIDS;


-- 그룹권한관리
CREATE TABLE TB_GRP_AUTH
(
	GROUP_CD varchar(20) NOT NULL,
	USER_NO varchar(20) NOT NULL,
	AUTHRT_ARTCL_CD varchar(3) NOT NULL,
	GROUP_AUTHRT varchar(20),
	RGTR_NO varchar(20),
	REG_YMD timestamp,
	MDFR_NO varchar(20),
	MDFCN_YMD timestamp,
	PRIMARY KEY (GROUP_CD, USER_NO, AUTHRT_ARTCL_CD)
) WITHOUT OIDS;


-- 그룹관리
CREATE TABLE TB_GRP_MNG
(
	GROUP_CD varchar(20) NOT NULL,
	BZENTY_NO varchar(20) NOT NULL,
	GROUP_NM varchar(100),
	RGTR_NO varchar(20),
	REG_YMD timestamp,
	MDFR_NO varchar(20),
	MDFCN_YMD timestamp,
	PRIMARY KEY (GROUP_CD)
