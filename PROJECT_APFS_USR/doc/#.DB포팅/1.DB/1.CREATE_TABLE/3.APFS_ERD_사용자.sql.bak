-- 그룹사용자관리
CREATE TABLE TB_GRP_USR
(
	GROUP_CD varchar(20) NOT NULL,
	USER_NO varchar(20) NOT NULL,
	RPRS_YN char(1),
	USE_STTS_CD varchar(3),
	RGTR_NO varchar(20),
	REG_YMD timestamp,
	MDFR_NO varchar(20),
	MDFCN_YMD timestamp,
	PRIMARY KEY (GROUP_CD, USER_NO)
) WITHOUT OIDS;


-- 투자분야코드관리
CREATE TABLE TB_INVTRLM_CODE
(
	INVT_FLD_CD varchar(10) NOT NULL,
	INVT_FLD_NM varchar(100),
	INVT_FLD_CN varchar(1000),
	ITM_KWRD varchar(300),
	USE_BGNG_YMD varchar(8),
	USE_END_YMD varchar(8),
	CD_ORDR numeric(5),
	USE_YN char(1),
	RGTR_NO varchar(20),
	REG_YMD timestamp,
	MDFR_NO varchar(20),
	MDFCN_YMD timestamp,
	PRIMARY KEY (INVT_FLD_CD)
) WITHOUT OIDS;


-- 사용자정보
CREATE TABLE TB_USER
(
	USER_NO varchar(20) NOT NULL,
	BZENTY_NO varchar(20),
	USER_ID varchar(50) UNIQUE,
	USER_NM varchar(40),
	PSWD varchar(100),
	EML_ADDR varchar(100),
	TELNO varchar(11),
	MBL_TELNO varchar(20),
	BRDT char(8),
	ZIP char(5),
	ADDR varchar(200),
	DADDR varchar(200),
	SEXDSTN char(1),
	DEPT_CD varchar(30),
	DEPT_NM varchar(200),
	EMPL_NO varchar(20),
	IDNTF_ID varchar(50),
	LGN_SE_CD varchar(3),
	JOIN_YMD varchar(8),
	WHDWL_YMD varchar(8),
	CNTN_PSBLTY_IP_ADDR varchar(80),
	PSWD_LOCK_YMD varchar(8),
	PSWD_ERR_CNT numeric(3),
	PSWD_CHG_YMD varchar(8),
	PSWD_NEXT_YMD varchar(8),
	LAST_LGN_DT timestamp,
	MOBLPHON_RCPTN_AGRE_YN char(1),
	PRVC_CLCT_AGRE_YN char(1),
	THPTY_PVSN_AGRE_YN char(1),
	USE_STTS_CD varchar(3),
	TEST_USE_YN char(1),
	RGTR_NO varchar(20),
	REG_YMD timestamp,
	MDFR_NO varchar(20),
	MDFCN_YMD timestamp,
	PRIMARY KEY (USER_NO)
) WITHOUT OIDS;



